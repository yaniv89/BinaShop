  <connectionStrings>
    <add name="DefaultConnection" connectionString="Data Source=localhost\SQLEXPRESS;Initial Catalog=BinaShop;Integrated Security=True" providerName="System.Data.SqlClient" />
  </connectionStrings>
  <appSettings>
    <!--Paypal logget settings-->

    <add key="PayPalLogger" value="PayPal.Log.Log4netLogger" />
    <add key="webpages:Version" value="3.0.0.0" />
